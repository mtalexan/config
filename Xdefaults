! For these settings to take effect, run "xrdb ~/.Xdefaults" to apply the
! changes, then re-run the program.

! Use xfontsel to look at x11 fonts (instead of truetype)
! Put it here:
!xterm*font: -*-terminus-*-*-*-*-14-*-*-*-*-*-*-*
! Alternatively specifiy a truetype font
xterm*faceName: Inconsolata:size=10:antialias=true
xterm*faceSize: 10

xterm*saveLines: 999999
!xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
! use a recognized name
xterm*termName: xterm-256color
xterm*eightBitInput: false
! Use system clipboard 
xterm*selectToClipboard: true
! Alt key works properly instead of being used for UTF-8 characters
xterm*metaSendsEscape: true
! disable scrollbar visibility
xterm*scrollBar: false

! See https://jonasjacek.github.io/colors/ for color to hex conversions
!  or /usr/share/emacs/*/etc/rgb.txt or /usr/share/vim/*/rgb.txt
! Colors can be explicitly specified in hex as "#rrggbb" or
!  as "rgb:rr/gg/bb", or by name

!Block Cursor
#define _color0        #000d18
#define _color8        #000d18
!RED Tag
#define _color1        #e89393
#define _color9        #e89393
!GRN SpecialKey
#define _color2        #9ece13
#define _color10       #9ece13
!YEL Keyword
#define _color3        #f0dfaf
#define _color11       #f0dfaf
!BLU Number
#define _color4        #8cd0d3
#define _color12       #8cd0d3
!MAG Precondit
#define _color5        #c0bed1
#define _color13       #c0bed1
!CYN Float
#define _color6        #dfaf8f
#define _color14       #dfaf8f
!WHT Search
#define _color7        #efefef
#define _color15       #efefef
!Bold, Underline, Italic with color, can be used with or instead of font change
#define _colorBDInstead false
#define _colorBD       #ffcfaf
#define _colorULInstead false
#define _colorUL       #ccdc90
#define _colorITInstead false
#define _colorIT       #80d4aa
!TXT Normal, Normal, Cursor
#define _foreground    chartreuse
!#dcdccc
#define _background    black
!#1f1f1f
#define _cursorColor   red
!#8faf9f

rxvt*color0         : _color0
